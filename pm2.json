{
  "apps": [
    {
      "name": "vinema_api",
      "script": "./bin/www.js",
      "watch": ["bin"],
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "instances"  : 3,
      "exec_mode"  : "cluster",
      "ignore_watch": ["node_modules", "logs", "src", "setup"]
    }
  ]
}